generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Token {
  id         String    @id @default(cuid())
  hash       String    @unique
  expiresAt  DateTime?
  createdAt  DateTime  @default(now())
  cupId      String    @default("") @map("cup_id")
  isApproved Boolean   @default(false)
  name       String    @default("")

  @@index([hash])
}
